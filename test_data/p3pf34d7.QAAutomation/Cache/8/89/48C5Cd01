/*  "* {margin:0; padding:0;}" messes up form controls in some browsers
    so instead we do it on specific elements */
body, html, form, fieldset, td, th,
dl, li, blockquote, h1, h2, h3, h4, h5, h6 {margin:0; padding:0;}
p {margin: 1em 0; font-size: 0.9em}
ul, ol {margin: 1em 0 1em 2em; padding: 0}
img {border:0;}
body {
  font: 12.5px/1.5em Arial, sans-serif;
  color:#464646;
}
a:active, a:focus, input:focus {outline:none;}
a {color:#0054a6;}
h3, h4 {font-size: 1.2em;}
h5 {font-size: 1em;margin: 1em 0 0;}
table {font-size: inherit;}

